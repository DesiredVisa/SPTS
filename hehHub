S3:NewToggle("Get Purple Aura (Respawn)", "Info", function(state)
    da = state
    if da then
        task.wait(0.2)
        pcall (function()
            game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
                char.DescendantAdded:Connect(function(obj)
                  if obj.Name == "Aura2" then
                    game:GetService("Debris"):AddItem(obj, 0)
                  end
                end)
            end)
        end)
    end
end)


S4:NewToggle("TP To Nick", "Info", function(state)
    da = state
    while da do
        task.wait(0.2)
        pcall (function()
            local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
            local p2 = "vHxpnotixx"
            p1.CFrame = game:GetService("Players")[p2].Character.HumanoidRootPart.CFrame
        end)
    end
end)

